{
  "expo": {
    "name": "Need India",
    "slug": "need-india",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./assets/images/icon100x100.png",
    "splash": {
      "image": "./assets/images/icon100x100.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "needindia",
    "android": {
      "package": "com.needindia25.needindia"
    }
  }
}
